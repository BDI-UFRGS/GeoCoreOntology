    <!-- https://www.inf.ufrgs.br/bdi/ontologies/geocore.owl#UFRGS:GeoCoreOntology_age_of -->

    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/bdi/ontologies/geocore.owl#UFRGS:GeoCoreOntology_age_of">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/RO_0000080"/>
        <owl:inverseOf rdf:resource="https://www.inf.ufrgs.br/bdi/ontologies/geocore.owl#UFRGS:GeoCoreOntology_has_age"/>
        <obo:IAO_0000115 xml:lang="en">The relation between a Geological Age and a Geological Object. Inverse of has age relation.</obo:IAO_0000115>
